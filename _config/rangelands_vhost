<VirtualHost *:80>
        ServerName rangelands.local
        ServerAdmin info@rangelands.local

        DocumentRoot "/var/www/html/rangelands"

        <Directory "/var/www/html/rangelands/">
                Options All
                AllowOverride All
                Order allow,deny
                Allow from all
        </Directory>

        ErrorLog "/var/log/apache2/error_log"
        TransferLog "/var/log/apache2/access_log"
</VirtualHost>
